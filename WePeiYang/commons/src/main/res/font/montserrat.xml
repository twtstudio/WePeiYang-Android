    <font
        android:fontStyle="normal"
        android:fontWeight="300"
        android:font="@font/montserrat_light"
        app:fontStyle="normal"
        app:fontWeight="300"
        app:font="@font/montserrat_light"/>

    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/montserrat_regular"
